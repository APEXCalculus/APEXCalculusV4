              \addplot [tangentlineseg,domain=0.75:1.25]{3*(x-1)+1.5};
              \addplot [tangentlineseg,domain=0.75:2]{1/3*(x-1.5)+1};
              \addplot [soliddot] coordinates{(-.5,.375)} node[above left] {$(-0.5,0.375)$};
