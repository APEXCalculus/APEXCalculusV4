                triple g(real t) {return (2*cos(t)+2,2*sin(t),0);}
                path3 mypath=graph(g,0,2*pi,operator ..);
                draw(mypath,blue+linewidth(2));
                triple g(real t) {return (cos(t)+1,sin(t),0);}
                path3 mypath=graph(g,0,2*pi,operator ..);
                draw(mypath,blue+linewidth(1));

