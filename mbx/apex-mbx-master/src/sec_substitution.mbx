                          xmin=-1.5,xmax=5.5]

              \addplot+[domain=-1.5:5.5,smooth,thick,samples=50] {cos(deg(3*x-1))} node[pos=0.54, above right]{\scriptsize $y=\cos(3x-1)$};
              \addplot [{\coloronefill},thick,fill={\coloronefill},domain=0:2] {cos(deg(3*x-1))} \closedcycle;
              \end{axis}

              \end{tikzpicture}
              ]]>
            </latex-image-code>
          </image>
        </figure>
          <!-- figures/fig_subst12a.tex END -->
      <figure xml:id="fig_subst12b">
        <caption/>
          <!-- START figures/fig_subst12b.tex -->
          <image xml:id="img_subst12b">
            <description></description>
            <latex-image-code>
              <![CDATA[
              \begin{tikzpicture}
              \begin{axis}[axis on top,
                           xtick={-1,1,2,3,4,5},
