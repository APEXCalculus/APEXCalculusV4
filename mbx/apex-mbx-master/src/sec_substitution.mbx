                           xmin=-.5,xmax=2]

              \addplot+[smooth,thick,domain=-.5:2] {cos(deg(x))*sin(deg(x)} node[pos=0.5, above right]{\scriptsize $y=\sin(x) \cos(x)$};
              \addplot [{\coloronefill},thick,fill={\coloronefill},domain=0:1.57] {cos(deg(x))*sin(deg(x)}\closedcycle;
              \end{axis}
              \end{tikzpicture}
              ]]>
            </latex-image-code>
          </image>
        </figure>
          <!-- figures/fig_subst13a.tex END -->
         
         <figure xml:id="fig_subst13b">
