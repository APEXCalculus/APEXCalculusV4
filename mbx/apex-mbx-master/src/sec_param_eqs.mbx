                  my $eliminated=Formula("($xstu/2)^2 + ($ystu/2)^2");
                  return 0 unless ($eliminated == Formula("1"));

                  #normalize answers
                  $xstu=$xstu/2;
                  $ystu=-$ystu/2;
                  $xcor=$xcor/2;
                  $ycor=-$ycor/2;

                  #shift student answers
                  my $xstu0=$xstu->eval(t=>0);
                  my $ystu0=$ystu->eval(t=>0);
                  my $phi=($xstu0 >= 0)?Compute("arcsin($ystu0)"):Compute("pi-arcsin($ystu0)");
                  $xstu=$xstu->substitute(t=>Formula("t-$phi"));
                  $ystu=$ystu->substitute(t=>Formula("t-$phi"));

                  return ($xstu == $xcor and $ystu == $ycor);
                }
              );
            </pg-code>
          </setup>
