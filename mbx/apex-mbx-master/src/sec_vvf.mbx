			xmin=-4.9,xmax=4.9%
                ]


                \addplot [thick,{\colorone}, smooth,domain=-10:10] ({.2*x},{.3*x});
                \addplot [thick,{\colorone}, smooth,domain=0:360] ({cos(x)},{sin(x)});
                %\addplot [thick,{\colorone}, smooth,domain=-10:10,samples=40] ({.2*x+cos(deg(x))},{.3*x+sin(deg(x))});



                \end{axis}



                \end{tikzpicture}
                ]]>
              </latex-image-code>
            </image>
            <!-- figures/fig_vvf3.tex END -->
          </figure>

          <figure>
            <caption>(b)</caption>
            <!-- START figures/fig_vvf3a.tex -->
            <image xml:id="img_vvf3a" >
              <description></description>
              <latex-image-code>
